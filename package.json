    "morgan": "~1.9.1",
    "nodemon": "^2.0.4",
    "axios": "^0.26.0",
    "concurrently": "^7.0.0",
    "cors": "^2.8.5",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "fs-extra": "^10.0.0",
    "mongodb": "^4.3.1",
    "multer": "^1.4.4",
    "react-player": "^2.9.0"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "constantinople": ">=3.1.1"
  }
